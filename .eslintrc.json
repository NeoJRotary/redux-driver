{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "no-param-reassign": 0,
    "comma-dangle": [2, "never"],
    "import/extensions": [
      2, "never",
      {
        "jsx": "always",
        "json": "always"
      }
    ]
  },
  "globals": {
    "google": true
  }
}
